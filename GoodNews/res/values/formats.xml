<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- date formats -->
    <!-- supported place holders
        {0}: date object
        {1}: full date (2012/20/24)
        {2}: time (5:49 pm)
    -->
    <string name="format_date_today">today {2}</string>
    <string name="format_date_week_short">{0,date,EEE} {2}</string>
    <string name="format_date_week_long">{0,date,EEE} {2}</string>
    <string name="format_date_year_short">{0,date,MMM d}</string>
    <string name="format_date_year_long">{0,date,MMMM d} {2}</string>
    <string name="format_date_past_short">{1}</string>
    <string name="format_date_past_long">{1} {2}</string>
</resources>